@section Scripts
{
    <script src="~/lib/signalr/dist/browser/signalr.min.js"></script>
    <script src="~/js/monitoringParser.js"></script>

}

<div style="background-color:aqua; float:left">
    <form method="post" enctype="multipart/form-data" asp-controller="NodeLang" asp-action="UploadWorkFlow">
        <div class="form-group">
            <div class="col-md-10">
                <p>Upload one or more files using this form:</p>
                <input type="file" name="file" multiple>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-10">
                <input type="submit" value="Deploy">
            </div>
        </div>
    </form>

    <button style="float:left;" onclick="readXml()">show</button>
    <button style="float:left;" onclick="change()">change</button>
</div>
<div style="background-color:gold;float:left">
    <svg id="WorkFlow" width="1000" height="1000" xmlns="http://www.w3.org/2000/svg">
        <defs>
            <marker id="arrowhead" viewBox="0 0 20 20" refX="11" refY="10" markerUnits="strokeWidth" markerWidth="10" markerHeight="10" orient="auto">
                <path d="M 1 5 L 11 10 L 1 15 Z" style="fill: black; stroke-width: 1px; stroke-linecap: round; stroke-dasharray: 10000, 1; stroke: black;"></path>
            </marker>
        </defs>
    </svg>
</div>