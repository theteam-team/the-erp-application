            ],
            "styles": [
              "ProductManager/styles.css"
            ],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "ProductManager/environments/environment.ts",
                  "with": "ProductManager/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
